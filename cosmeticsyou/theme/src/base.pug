| {% load static %}
doctype
html.no-js(lang="ru")
head
  meta(charset="UTF-8")
  meta(
    name="viewport",
    content="width=device-width, initial-scale=1.0"
  )
  meta(
    http-equive="X-UA-Compatible",
    content="IE=edge"
  )
  | {% block title  %}
  | {% endblock %}
  // Header and Footer
  style.
    .header {
        margin-top: 20px
    }
    .header__navbar {
        background-color: transparent;
        border: 0
    }
    .navContent__logo {
        width: 32.57576%;
        max-width: 320px
    }
    @media (min-width: 769px) and (max-width:991px) {
        .navContent__logo {
            width:99.24242%
        }
    }
    @media (max-width: 769px) {
        .navContent__logo {
            width:99.24242%;
            margin: 0 auto!important
        }
    }
    #logo {
        width: 100%;
        height: auto
    }
    @media (max-width: 769px) {
        .logo__navButton {
            float:none;
            display: block;
            margin: 0 auto
        }
    }
    .navContent__navigation {
        width: 65.90909%;
        float: right;
        margin-top: 30px
    }
    @media (max-width: 1199px) {
        .navContent__navigation {
            width:99.24242%;
            float: left;
            margin-left: .37879%;
            margin-right: .37879%;
            padding-left: 0;
            font-size: .9375em
        }
    }
    @media (max-width: 768px) {
        .navContent__navigation {
            width:99.24242%;
            float: left;
            margin-left: .37879%;
            margin-right: .37879%;
            text-align: center;
            margin-left: auto!important;
            margin-right: auto!important;
            padding: 0
        }
    }
    .navigation__navList {
        font-size: 1em
    }
    .navigation__navList .active .navItem__refer {
        color: #fff!important;
        background-color: #000!important
    }
    @media (max-width: 1199px) {
        .navigation__navList {
            float:left!important
        }
    }
    @media (max-width: 768px) {
        .navigation__navList {
            float:none;
            width: 99.24242%;
            margin-left: 0;
            margin-right: 0
        }
    }
    .navList__navItem:hover .navItem__refer {
        color: #fff!important;
        background-color: #000!important
    }
    @media (max-width: 768px) {
        .navList__navItem {
            width:99.24242%
        }
    }
    .navItem__refer {
        -webkit-transition: color 1s,background 1s;
        transition: color 1s,background 1s
    }
    .navigation__btnTop {
        position: fixed;
        bottom: 2vw;
        right: 5vw;
        z-index: 999
    }
    .navigation__btnTop i {
        color: #3f3c39
    }
    .dropdown-menu {
        min-width: 133px
    }
    @media (min-width: 1200px) {
        .dropdown-menu {
            min-width:137px
        }
    }
    @media (max-width: 787px) {
        .dropdown-menu {
            text-align:center
        }
    }
    .navList__navItem:hover .dropdown-menu .navItem__refer {
        color: #fff!important
    }
    .footer {
        border-top-style: double;
        border-top-width: 3px;
        border-color: #eee;
        padding-top: 5%;
        padding-bottom: 5%;
        font-size: .875em;
        color: #827c76
    }
    @media (max-width: 460px) {
        .footer {
            padding-top:10%;
            padding-bottom: 15%
        }
    }
    .contacts {
        width: 57.57576%;
        float: left;
        margin-left: .37879%;
        margin-right: .37879%;
        padding-bottom: 4%
    }
    @media (max-width: 769px) {
        .contacts {
            width:40.90909%;
            float: left;
            margin-left: .37879%;
            margin-right: .37879%
        }
    }
    @media (max-width: 460px) {
        .contacts {
            width:99.24242%;
            float: left;
            margin-left: .37879%;
            margin-right: .37879%;
            margin-top: 35px;
            text-align: center;
            padding-bottom: 10%
        }
    }
    .address__item,.email__item,.officialSite__refer,.submitForm__agreement:hover,.submitForm__conditions:hover,.tel__item {
        -webkit-transition: color .5s;
        transition: color .5s;
        color: #3f3c39
    }
    .address__item:hover,.email__item:hover,.officialSite__refer:hover,.submitForm__agreement:hover,.submitForm__conditions:hover,.tel__item:hover {
        color: #bd0d0d!important
    }
    .address__item:visited,.email__item:visited,.officialSite__refer:visited,.submitForm__agreement:visited:hover,.submitForm__conditions:visited:hover,.tel__item:visited {
        color: #3f3c39
    }
    .contacts__item {
        margin-top: 20px
    }
    .address__label,.tel__label,email__label {
        font-size: 1.125em
    }
    .info {
        width: 32.57576%;
        float: left;
        margin-left: .37879%;
        margin-right: .37879%;
        margin-right: 4.54545%
    }
    @media (max-width: 769px) {
        .info {
            width:49.24242%;
            float: left;
            margin-left: .37879%;
            margin-right: .37879%;
            margin-right: 4.54545%
        }
    }
    @media (max-width: 460px) {
        .info {
            width:99.24242%;
            float: left;
            margin-left: .37879%;
            margin-right: .37879%;
            margin-right: 0;
            padding: 0 13%
        }
    }
    .officialSite {
        margin-top: 20px
    }
    .copyright {
        padding: 4% 0 3%;
        border-top: 1px solid #eee
    }
    @media (max-width: 460px) {
        .copyright {
            padding-top:10%
        }
    }
    .curtains, .preloader {
      width: 100%;
      height: 100%;
      position: fixed;
    }
    .curtains {
      top: 0;
      left: 0;
      background-color: #fff;
    }
    
    .preloader {
      background: url("{% static 'cosmeticsyou/img/loader/ajax-loader.svg' %}") center no-repeat;
      background-size: 100px;
    }
    
  script(async, src="{% static 'cosmeticsyou/js/modernizr.js' %}") 
  
body
  .curtains
    figure.preloader
  include blocks/header/callback/callback.pug
  // Main Content
  | {% block content %}
  | {% endblock %}
  // Footer
  include blocks/footer/footer.pug
  
